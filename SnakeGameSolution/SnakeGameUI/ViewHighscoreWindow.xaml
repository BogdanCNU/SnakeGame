<Window x:Class="SnakeGameUI.ViewHighscoreWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeGameUI"
        mc:Ignorable="d"
        Title="Highscore" Height="140" Width="250" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="D:/Snake.png" Stretch="None"/>
        </Grid.Background>
        <TextBlock Text="Best Score" HorizontalAlignment="Center" FontSize="22" FontWeight="Bold"/>
    </Grid>
</Window>
